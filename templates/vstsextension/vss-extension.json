{
  "manifestVersion": 1,
  "id": "new-extension-id",
  "version": "0.1.0",
  "name": "New Extension",
  "description": "A brand new extension generated by yo vsts-extension.",
  "baseUri": "https://localhost:44300",
  "publisher": "yourpublisherid",
  "contributions": [
    {
      "id": "new-extension-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "The top level hub group",
      "targets": [
        "ms.vss-web.project-hub-groups-collection"
      ],
      "properties": {
        "name": "New Hub Group",
        "order": 100
      }
    },
    {
      "id": "new-extension-hub",
      "type": "ms.vss-web.hub",
      "description": "The second level hub.",
      "targets": [
        ".new-extension-hub-group"
      ],
      "properties": {
        "name": "New Hub",
        "order": 100,
        "uri": "hub.html"
      }
    }
  ]
}
